{
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  },
  "opts": {
    "destination": "./docs/jsdoc",
    "recurse": true,
    "readme": "../README.md",
    "encoding": "utf8",
    "template": "node_modules/docdash"
  },
  "metadata": {
    "title": "Camping Reservation Notifier API",
    "description": "Backend API documentation for camping reservation notification system"
  }
}
